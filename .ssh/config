# github account
AddKeysToAgent yes
Host ghe
	HostName camaero.ghe.com
	User camaero
	IdentityFile ~/.ssh/ca
	IdentitiesOnly yes
  AddKeysToAgent yes

Host github.com
	IdentitiesOnly=yes
	HostName github.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/personal

Host github.com-work
	IdentitiesOnly=yes
	HostName github.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/github_malloy

